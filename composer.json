{
    "name": "iresults/pictures",
    "type": "typo3-cms-extension",
    "description": "Another gallery extension",
    "authors": [
        {
            "name": "Andreas Thurnheer-Meier",
            "role": "Developer"
        },
        {
            "name": "Daniel Corn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-exif": "*",
        "prewk/result": "^2.1.1"
    },
    "require-dev": {
        "vimeo/psalm": "^3.11.2"
    },
    "autoload": {
        "psr-4": {
            "Iresults\\Pictures\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iresults\\Pictures\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/pictures": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "pictures"
        }
    }
}
